    <sql>select mt.Transaction_Code &#x22;Code Transaction&#x22; ,  &#x22;MT&#x22; as &#x22;Produit&#x22; , 25 as &#x22;Product ID&#x22; , &#xd;&#xa;DATE_FORMAT&#x28;mt.payment_date,&#x22;&#x25;Y-&#x25;m-&#x25;d&#x22;&#x29; as &#x22;date&#x22; , ag.code  as &#x22;CodeAgence&#x22;,&#xd;&#xa;round&#x28;sum&#x28;transfer_value&#x29;&#x29; as &#x22;Montant&#x22; , &#x22;Ajout&#x22; as &#x22;Sens&#x22; &#xd;&#xa;from mt_daily_activity mt left join  agencies ag &#xd;&#xa;on ag.code &#x3d; mt.payment_branch_code&#xd;&#xa;where DATE_FORMAT&#x28;mt.payment_date,&#x22;&#x25;Y-&#x25;m-&#x25;d&#x22;&#x29; &#x3d; DATE_ADD&#x28;DATE_FORMAT&#x28;now&#x28;&#x29;,&#x22;&#x25;Y-&#x25;m-&#x25;d&#x22;&#x29;,INTERVAL -1 day&#x29; and&#xd;&#xa; mt.Transaction_Code not in &#x28;select transaction_code&#xd;&#xa;          from cp_daily_activity  where STR_TO_DATE&#x28;date_time , &#x22;&#x25;d&#x2f;&#x25;m&#x2f;&#x25;Y&#x22;&#x29; &#x3d; DATE_ADD&#x28;DATE_FORMAT&#x28;now&#x28;&#x29;,&#x22;&#x25;Y-&#x25;m-&#x25;d&#x22;&#x29;,INTERVAL -1 day&#x29;  and product_id &#x3d; 25 &#x29;&#xd;&#xa;&#xd;&#xa;&#x3b;</sql>
