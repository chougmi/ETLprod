     where id_agent = 2
     group by  cin,code ,agence,status , id_agent,agent,nom) sales
     on sales.code = shopkeeper_realisations.sShopId
     where producttype_id   in( 1,2) 
     and month(dinsertion) = MONTH(getdate()) and year(dinsertion) = year(getdate())
     group by  cin,nom, month(dinsertion) ,year(dinsertion)

union 


     select cin,year(dinsertion)as annee,month(dinsertion) as mois , 22 as 'producttype_id','Facturiers' as type_product  , nom as 'sup'  ,sum(nbr)as valeur_calcule  from shopkeeper_realisations 
     left join (select cin,code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
     where id_agent = 2
      group by  cin,code ,agence,status , id_agent,agent,nom) sales
     on sales.code = shopkeeper_realisations.sShopId
     where producttype_id   in( 22,12)  
     and month(dinsertion) = MONTH(getdate()) and year(dinsertion) = year(getdate())
     group by cin, nom, month(dinsertion),year(dinsertion))  realisation

left join 
     (select cin,annee, mois ,3 as 'product_id' , nom as 'sup'  ,sum(objectif_nbr) as nbr from objectif_sales obj
     left join (select cin, code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
     where id_agent = 2
     group by cin, code ,agence,status , id_agent,agent,nom) sales
     on sales.code = code_agence
     where product_id in (3,23) and  mois  = MONTH(getdate()) and annee = year(getdate())
     group by cin, nom ,mois ,annee

union 

      select cin, annee, mois ,product_id , nom as 'sup'  ,sum(objectif_nbr) as nbr from objectif_sales obj
      left join (select cin, code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
      where id_agent = 2
      group by  cin,code ,agence,status , id_agent,agent,nom) sales
      on sales.code = code_agence
      where product_id in (1,2)  and  mois  = MONTH(getdate()) and annee = year(getdate())
      group by  cin,nom ,mois ,product_id,annee

union

     select cin,annee,mois ,product_id , nom as 'sup'  ,sum(objectif_nbr) as nbr from objectif_sales obj
     left join (select cin, code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
     where id_agent = 2
     group by  cin,code ,agence,status , id_agent,agent,nom) sales
     on sales.code = code_agence
     where product_id in (22,12) and  mois  = MONTH(getdate())and annee = year(getdate())
     group by  cin,nom ,mois ,product_id,annee

) budget
 on realisation.mois = budget.mois and realisation.producttype_id = budget.product_id  and realisation.sup = budget.sup


and realisation.annee = budget.annee
--------------------------------------------
union



select  ''as cin ,5 as id,realisation.annee, realisation.mois as Mois ,cast(realisation.sShopId as varchar) as agent ,realisation.type_product as Type_product ,realisation.valeur_calcule as Realisation , budget.nbr as Objectif
 , realisation.valeur_calcule/ NULLIF(budget.nbr,0)as Tro

from (

select year(dinsertion) annee, month(dinsertion) as mois , 3 as producttype_id,'international' as type_product  , sShopId ,sum(nbr)as valeur_calcule  from shopkeeper_realisations 
where producttype_id   in(3) 
and month(dinsertion) = MONTH(getdate()) and year(dinsertion) = year(getdate())
group by  sShopId, month(dinsertion), sShopId, year(dinsertion)

union 

select year(dinsertion) annee, month(dinsertion) as mois , 1 as  producttype_id,'national'as type_product  , sShopId ,sum(nbr)as valeur_calcule  from shopkeeper_realisations 
where producttype_id   in( 1,2) 
and month(dinsertion) = MONTH(getdate()) and year(dinsertion) = year(getdate())
group by  sShopId, month(dinsertion) , sShopId, year(dinsertion)


union 
select  year(dinsertion) annee, month(dinsertion) as mois , 22 as  'producttype_id','Facturiers' as 'type_product'  , sShopId ,sum(nbr)as valeur_calcule  from shopkeeper_realisations 
where producttype_id   in( 22,12) 
and month(dinsertion) = MONTH(getdate())  and year(dinsertion) = year(getdate())
group by  sShopId, month(dinsertion)  , sShopId,year(dinsertion))realisation
left join (
select  annee,mois , 3 as product_id , code_agence ,sum(objectif_nbr) as nbr from objectif_sales
where product_id in (3) and mois  = MONTH(getdate())  and annee = year(getdate())
group by  code_agence ,mois ,annee  

union 
select annee, mois ,product_id , code_agence ,sum(objectif_nbr) as nbr from objectif_sales
where product_id in (1,2) and mois  = MONTH(getdate())  and annee = year(getdate())
group by  code_agence  ,mois ,product_id,annee
union 
select annee, mois ,22 as 'product_id', code_agence ,sum(objectif_nbr) as nbr from objectif_sales
where product_id in (22,12) and mois  = MONTH(getdate())  and annee = year(getdate())
group by  code_agence ,mois  ,annee
--union 
--select annee,mois ,product_id , code_agence ,sum(objectif_nbr) as nbr from objectif_sales
--where product_id in (23) and mois  = MONTH(getdate())
--group by  code_agence  ,mois ,product_id,annee

--union
--select annee, mois ,product_id , code_agence  ,sum(objectif_nbr) as nbr from objectif_sales obj
--where product_id in (89) and  mois  = MONTH(getdate())
--group by annee, code_agence ,mois ,product_id



)  budget
 on realisation.mois = budget.mois and realisation.producttype_id = budget.product_id  and realisation.sShopId = budget.code_agence
and realisation.annee = budget.annee 

