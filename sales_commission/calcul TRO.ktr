    <sql>select '4' as id,realisation.annee,realisation.mois as Mois ,realisation.regional as agent ,realisation.type_product as Type_product ,realisation.valeur_calcule as Realisation , budget.nbr as Objectif
 , realisation.valeur_calcule/ NULLIF(budget.nbr,0)as Tro from (


select year(dinsertion)as annee,month(dinsertion) as mois , 3 as 'producttype_id','International'type_product  , nom as 'regional' ,sum(nbr)as valeur_calcule  from shopkeeper_realisations 
left join (select code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
where id_agent = 4
group by  code ,agence,status , id_agent,agent,nom) sales
on sales.code = shopkeeper_realisations.sShopId
where producttype_id   in(3,23,58) and  month(dinsertion) =    MONTH(getdate())-1

group by  nom, month(dinsertion),year(dinsertion)
union 

select year(dinsertion)as annee, month(dinsertion) as mois , 1 as 'producttype_id', 'national' as type_product  , nom as 'regional' ,sum(nbr)as valeur_calcule  from shopkeeper_realisations 
left join (select code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
where id_agent = 4
group by  code ,agence,status , id_agent,agent,nom) sales
on sales.code = shopkeeper_realisations.sShopId
where producttype_id   in( 1,2) and month(dinsertion) =   MONTH(getdate())-1

group by  nom, month(dinsertion) ,year(dinsertion)

union 
select year(dinsertion)as annee, month(dinsertion) as mois , 22 as 'producttype_id','Facturiers' as type_product  , nom as 'regional' ,sum(nbr)as valeur_calcule  from shopkeeper_realisations 
left join (select code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
where id_agent = 4
group by  code ,agence,status , id_agent,agent,nom) sales
on sales.code = shopkeeper_realisations.sShopId
where producttype_id   in( 22,12)  and month(dinsertion) =   MONTH(getdate())-1

group by  nom, month(dinsertion),year(dinsertion))  realisation

left join 
(select annee, mois ,3 as 'product_id' , nom as 'regional' ,sum(objectif_nbr) as nbr from objectif_sales obj
left join (select code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
where id_agent = 4
group by  code ,agence,status , id_agent,agent,nom) sales
on sales.code = code_agence
where product_id in (3,23)  and mois  =   MONTH(getdate())-1
group by  nom ,mois ,annee

union 

select annee,mois ,product_id , nom as 'regional' ,sum(objectif_nbr) as nbr from objectif_sales obj
left join (select code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
where id_agent = 4
group by  code ,agence,status , id_agent,agent,nom) sales
on sales.code = code_agence
where product_id in (1,2)and mois  =   MONTH(getdate())-1
group by  nom ,mois ,product_id,annee

union

select annee,mois ,product_id ,  nom as 'regional' ,sum(objectif_nbr) as nbr from objectif_sales obj
left join (select code ,agence,status , id_agent,agent,nom ,max(date_affectation) date_affectation from sales_affectation
where id_agent = 4
group by  code ,agence,status , id_agent,agent,nom) sales
on sales.code = code_agence
where product_id in (22,12) and mois  =   MONTH(getdate())-1
group by  nom ,mois ,product_id,annee

) budget
 on realisation.mois = budget.mois and realisation.producttype_id = budget.product_id  and realisation.regional = budget.regional

 and  realisation.annee = budget.annee

union




select  '2' as id ,realisation.annee,realisation.mois as Mois ,realisation.ccs as agent ,realisation.type_product as Type_product ,realisation.valeur_calcule as Realisation , budget.nbr as Objectif
 , realisation.valeur_calcule/ NULLIF(budget.nbr,0)as Tro from (


