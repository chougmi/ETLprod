    <sql> 

  select
   created_date as ' date enrolement' ,  cin  as'N doc enrolement' , sender_firstname as 'firstname' , sender_lastname as'lastname' , sex as 'Sexe' ,
			dbirthday as 'Birthday' , sgsm	 , card_number as 'card_number cartonnée' ,	
office_code as'code agence enrolement cartonnée' , reseau 	as'Type (FR/PR)' , shop_city	 , count(*) as'Nbr de visites'	 ,
fidelity_balance as'Solde fidélité'	  , sum(converted_points) as'Nombre de points convertis' ,
0 as 'Carte biométrique ( 0/1)'	, 0 as 'code agence enrolement biometrique'  , '' as'date enrolement biometrique'
from fdlt_fnl 
left join dw_dim_shop on office_code = shopcode 
where  
  cin not in (select  cin from fdlt_fnl where  code_card &lt;&gt; 'fide'  )
group by 

 created_date  ,  cin  ,sender_firstname  , sender_lastname  , sex 
		 ,	dbirthday , sgsm	 , card_number ,	
office_code  , reseau ,	shop_city	  ,	 
fidelity_balance

union 


  select
   f1.created_date as ' date enrolement' ,  f1.cin  as'N doc enrolement' , f1.sender_firstname as 'firstname' , f1.sender_lastname as'lastname' , f1.sex as 'Sexe' ,
			f1.dbirthday as 'Birthday' , f1.sgsm	 , f1.card_number as 'card_number cartonnée' ,	
f1.office_code as'code agence enrolement cartonnée' , reseau 	as'Type (FR/PR)' , shop_city	 , count(*) as'Nbr de visites'	 ,
f1.fidelity_balance as'Solde fidélité'	  , sum(f1.converted_points) as'Nombre de points convertis' ,
1 as 'Carte biométrique ( 0/1)'	, f2.office_code as 'code agence enrolement biometrique'  ,  f2.date  as'date enrolement biometrique'
from fdlt_fnl f1 inner join  fdlt_bio  f2 on f1.cin = f2.cin
left join dw_dim_shop on f1.office_code = shopcode 
 
  where code_card = 'fide'
group by 

 f1.created_date  ,  f1.cin  ,f1.sender_firstname  , f1.sender_lastname  , f1.sex 
		 ,	f1.dbirthday , f1.sgsm	 , f1.card_number ,	
f1.office_code  ,reseau ,	shop_city	  ,	 
f1.fidelity_balance , f2.date, f2.office_code</sql>
