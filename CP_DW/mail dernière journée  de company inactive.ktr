    <name>Extraction depuis table 2</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>bi</connection>
    <sql>select a.sname as Company,
			  (select TOP 1 b.maxdate from Rincative b where a.sname = b.sname and type ='facturation'  order by b.maxdate DESC) as dernière_journée_facturations,
			  (select TOP 1 sum(c.nAmount) from Rincative c where a.sname = c.sname and type ='facturation' group by c.maxdate,c.type  order by c.maxdate DESC) as volume_facturations,
			  (select TOP 1 e.maxdate from Rincative e where a.sname = e.sname and type ='Transactions'  order by e.maxdate DESC) as dernière_journée_Transactions,
			  (select TOP 1 sum(d.nAmount) from Rincative d where a.sname = d.sname and type ='Transactions'group by d.maxdate,d.type order by d.maxdate DESC) as Balance_Transactions,
		    (select TOP 1 f.maxdate from Rincative f where a.sname = f.sname and type ='Alimentatio ncompany'  order by f.maxdate DESC) as dernière_journée_Alimentation,
			  (select TOP 1 sum(g.nAmount) from Rincative g where a.sname = g.sname and type ='Alimentatio ncompany'group by g.maxdate,g.type  order by g.maxdate DESC) as volume_Alimentation,
		    (select TOP 1 h.maxdate from Rincative h where a.sname = h.sname and type ='Interagence'  order by h.maxdate DESC) as dernière_journée_Interagence,
        (select TOP 1 sum(k.nAmount) from Rincative k where a.sname = k.sname and type ='Interagence' group by k.maxdate,k.type  order by k.maxdate DESC) as volume_Interagence,
	      (select TOP 1 m.maxdate from Rincative m where a.sname = m.sname and type ='Versement virement banque' order by m.maxdate DESC) as dernière_journée_Versement,
        (select TOP 1 sum(n.nAmount) from Rincative n where a.sname = n.sname and type ='Versement virement banque' group by n.maxdate,n.type order by n.maxdate DESC) as volume_Versement ,
			  (select TOP 1 l.maxdate from Rincative l where a.sname = l.sname and type ='Correction'  order by l.maxdate DESC) as dernière_journée_Correction,
        (select TOP 1 sum(v.nAmount) from Rincative v where a.sname = v.sname and type ='Correction' group by v.maxdate,v.type  order by v.maxdate DESC) as volume_Correction,
				
				(select TOP 1 q.maxdate from Rincative q where a.sname = q.sname and type = 'Change Devise'  order by q.maxdate DESC) as dernière_journée_Change_Devise ,
        (select TOP 1 sum(t.nAmount) from Rincative t where a.sname = t.sname and type = 'Change Devise' group by t.maxdate,t.type order by t.maxdate DESC) as volume_Change_Devise
			 
		from Company a
      where a.bactive = 0  



</sql>
