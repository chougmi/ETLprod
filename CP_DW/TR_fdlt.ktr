    <sql>


select 
 cast(tr.dInsertion as date)  as 'dInsertion' , 
cs.nid  as 'id client' ,

upper(p.sName) as Product ,


cast(sh.bfranchise as varchar(50)) as reseau, 
count(cs.scin)  as 'nombre de visite' ,
count(p.sName) as 'nombre de produit'



from 
Transactions "tr"   
left join 
            Shop "sh" on  cast(tr.nShopId as varchar(50))  = cast(sh.nId as varchar(50))  

left join  
            product "p"  on   tr.nproductId = p.nId
left  join  
                  TransactionStatus "t" on  tr.nTransactionStatusId = t.nId  
  left join customer  cs on  cs.nid =  tr.nSenderId 

where  

      
cast(tr.dInsertion as date) = '2019'
 and  nproductid = 1 and 
 tr.nTransactionStatusId in (8,26)
group by

cs.nid ,
cs.dbirthday ,

upper(p.sName) ,
cast(tr.dInsertion as date) ,




cast(sh.bfranchise as varchar(50))
 
union 

  select 
  cast(tr.dInsertion as date)  as '2019' , 
cs.nid  'id client',

upper(p.sName) as Product ,



cast(sh.bfranchise as varchar(50)) as reseau, 
count(cs.scin) as 'nombre de visite ' ,
count(p.sName) as 'nombre de produit'



from 
Transactions "tr"   
left join 
            Shop "sh" on  cast(tr.nShopId as varchar(50))  = cast(sh.nId as varchar(50))  

left join  
            product "p"  on   tr.nproductId = p.nId
left  join  
                  TransactionStatus "t" on  tr.nTransactionStatusId = t.nId  
  left join customer  cs on  cs.nid =  tr.nreceiverId 

where  

      
cast(tr.dInsertion as date) = '2019'
 and  nproductid &lt;&gt; 1 and 
 tr.nTransactionStatusId in (8,26)
group by
 cast(cs.dInsertion as date) ,

 cs.nid ,
upper(p.sName) ,

 cast(tr.dInsertion as date)  , 
cast(sh.bfranchise as varchar(50))
 


</sql>
