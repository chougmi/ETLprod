    <sql> select 'existe dans le  rapport et non existe dans   ms' as statut  ,Legacy_ID as code ,Tran_Date as date ,Reference_ID as codetrx , Base_Amt as amount 
from mg_base
left JOIN   Daily_transactions_extended
 on Daily_transactions_extended.product = 'Money Gram' and Daily_transactions_extended.sNenCode = CONVERT(varchar, CAST(mg_base.Reference_ID AS DECIMAL(38)))
where mg_base.Tran_Date = '2019-09-02'
and Daily_transactions_extended.dinsertion is null 

union all


select 'existe dans MS et non existe dans  rapport' as statut , sShopid  as code,  CONVERT(DATE,Daily_transactions_extended.dinsertion)  as date, sNenCode  as codetrx, nAmount as amount 

from Daily_transactions_extended
left JOIN   mg_base
 on    Daily_transactions_extended.sNenCode = CONVERT(varchar, CAST(mg_base.Reference_ID AS DECIMAL(38)))
 where  CONVERT(DATE,Daily_transactions_extended.dinsertion)  = '2019-09-02' and Daily_transactions_extended.product = 'Money Gram' 
 and mg_base.Tran_Date is null

 
 union all
 select 'Diff amount' as statut , sShopid  as code,  CONVERT(DATE,Daily_transactions_extended.dinsertion)  as date, sNenCode  as codetrx, (Daily_transactions_extended.namount + mg_base.Base_Amt) as amount

from Daily_transactions_extended
inner JOIN   mg_base
 on    Daily_transactions_extended.sNenCode = CONVERT(varchar, CAST(mg_base.Reference_ID AS DECIMAL(38)))
 where  CONVERT(DATE,Daily_transactions_extended.dinsertion)  = '2019-09-02'  and Daily_transactions_extended.product = 'Money Gram'  and (Daily_transactions_extended.namount + mg_base.Base_Amt) &lt;&gt; 0
</sql>
