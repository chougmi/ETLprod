    <sql>&#xd;&#xa;SELECT f.date,produit as product, sum&#x28;f.montant&#x29; as volume_r , sum&#x28;f.nombre&#x29; as nbr_r , sum&#x28;f.CA&#x29; as CA_r&#xd;&#xa;FROM dbo.euro_cube_2015 f&#xd;&#xa;where  f.produit  in &#x28;&#x27;CPIN&#x27;,&#x27;CPOUT&#x27;,&#x27;WU&#x27;,&#x27;MG&#x27;,&#x27;RIA&#x27;,&#x27;MT&#x27;,&#x27;ME&#x27;,&#x27;VI&#x27;,&#x27;RT&#x27;,&#x27;ASQ&#x27;,&#x27;VPS&#x27;&#x29;&#xd;&#xa;group by f.date,f.produit&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;union &#xd;&#xa;&#xd;&#xa;select t.date as date,&#x27;AUTRE&#x27; as product ,sum&#x28;t.volume_r&#x29; AS &#x27;volume_r&#x27;,sum&#x28;t.nbr_r &#x29; AS &#x27;nbr_r &#x27; ,sum&#x28;t.CA_r&#x29; as &#x27;CA_r&#x27;&#xd;&#xa;&#xd;&#xa;from &#x28;SELECT f.date ,f.produit  , sum&#x28;f.montant&#x29; as volume_r , sum&#x28;f.nombre&#x29; as nbr_r,sum&#x28;f.CA&#x29; as CA_r&#xd;&#xa;FROM dbo.euro_cube_2015 f&#xd;&#xa;where f.produit in &#x28;&#x27;MEP&#x27;,&#x27;MI&#x27;,&#x27;WARI&#x27;,&#x27;SW&#x27;,&#x27;MONTY&#x27;,&#x27;BLM&#x27;,&#x27;PINTAIL&#x27;,&#x27;EUROPHIL&#x27;,&#x27;TZ&#x27;,&#x27;TITANES&#x27;,&#x27;MUNDI&#x27;,&#x27;LINK&#x27;,&#x27;FE&#x26;ZE&#x27;,&#x27;VLT&#x27;,&#x27;SF&#x27;&#x29;&#xd;&#xa;group by f.date,f.produit&#xd;&#xa;&#x29; t&#xd;&#xa;group by t.date&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;union &#xd;&#xa;&#xd;&#xa;select date,&#x27;FACTURE&#x27; as product , sum&#x28;f.montant&#x29; as volume_r , sum&#x28;f.nombre&#x29; as nbr_r,sum&#x28;f.CA&#x29; as CA_r&#xd;&#xa;FROM dbo.euro_cube_2015 f&#xd;&#xa;&#xd;&#xa;where f.produit  in &#x28;&#x27;TGR&#x27;,&#x27;AMN Tanger&#x27;,&#x27;AMN Tetouan&#x27;,&#x27;DGI&#x27;,&#x27;LYDEC&#x27;,&#x27;INWI FACTURE&#x27;,&#x27;RAK&#x27;,&#x27;RADEEMA&#x27;,&#x27;RAMSA&#x27;&#x29;&#xd;&#xa;group by f.date,f.produit&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;UNION &#xd;&#xa;&#xd;&#xa;SELECT f.date,&#x27;CHANGE&#x27; as product, sum&#x28;f.montant&#x29; as volume_r , count&#x28;f.nombre&#x29; as nbr_r ,sum&#x28;f.CA&#x29; as CA_r&#xd;&#xa;FROM dbo.euro_cube_2015 f&#xd;&#xa;where f.produit in &#x28;&#x27;CHA&#x27;,&#x27;CHV&#x27;&#x29;&#xd;&#xa;group by f.date&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;order by date,product</sql>
