    <connection>local_cash_plus</connection>
    <sql>&#xd;&#xa;&#xd;&#xa;SELECT f.date,product , sum&#x28;f.Amount&#x29; as volume_r , count&#x28;&#x2a;&#x29; as nbr_r , sum&#x28;f.OperationFee&#x29; as CA_r&#xd;&#xa;FROM dbo.dw_fact_table f&#xd;&#xa;where  f.product  in &#x28;&#x27;CPIN&#x27;,&#x27;CPOUT&#x27;,&#x27;WU&#x27;,&#x27;MG&#x27;,&#x27;RIA&#x27;,&#x27;MT&#x27;,&#x27;ME&#x27;,&#x27;RT&#x27;,&#x27;VI&#x27;,&#x27;VPS&#x27;,&#x27;ASQ&#x27;&#x29;&#xd;&#xa;group by f.date,f.product&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;union &#xd;&#xa;&#xd;&#xa;select t.date as date,&#x27;AUTRE&#x27; as product,sum&#x28;t.volume_r&#x29; AS &#x27;volume_r&#x27;,sum&#x28;t.nbr_r &#x29; AS &#x27;nbr_r &#x27;,sum&#x28;t.CA_r&#x29; as &#x27;CA_r&#x27;&#xd;&#xa;from &#xd;&#xa;&#xd;&#xa;&#x28;SELECT f.date ,f.product , sum&#x28;f.Amount&#x29; as volume_r , count&#x28;&#x2a;&#x29; as nbr_r ,sum&#x28;f.OperationFee&#x29; as &#x27;CA_r&#x27;&#xd;&#xa;FROM dbo.dw_fact_table f&#xd;&#xa;where f.product not in &#x28;&#x27;CPIN&#x27;,&#x27;CPOUT&#x27;,&#x27;WU&#x27;,&#x27;MG&#x27;,&#x27;RIA&#x27;,&#x27;MT&#x27;,&#x27;ME&#x27;&#x29; and transactiontypeDetail&#x3d;&#x27;INTERNATIONAL&#x27;&#xd;&#xa;group by f.date,f.product&#x29; t&#xd;&#xa;group by t.date&#xd;&#xa;&#xd;&#xa;union &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT f.date,&#x27;FACTURE&#x27; as product, sum&#x28;f.Amount&#x29; as volume_r , count&#x28;&#x2a;&#x29; as nbr_r ,sum&#x28;f.OperationFee&#x29; as CA_r&#xd;&#xa;FROM dbo.dw_fact_table f&#xd;&#xa;where TransactionType &#x3d;&#x27;FACTURE&#x27;&#xd;&#xa;group by f.date&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;union&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT f.date,&#x27;CHANGE&#x27; as product, sum&#x28;f.Amount&#x29; as volume_r , count&#x28;&#x2a;&#x29; as nbr_r ,sum&#x28;f.OperationFee&#x29; as CA_r&#xd;&#xa;FROM dbo.dw_fact_table f&#xd;&#xa;where  TransactionType &#x3d;&#x27;CHANGE&#x27;&#xd;&#xa;group by f.date&#xd;&#xa;&#xd;&#xa;order by date,product&#xd;&#xa;&#xd;&#xa;</sql>
    <limit>0</limit>
