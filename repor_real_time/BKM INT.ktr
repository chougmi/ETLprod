    <sql>
  select 
        TAIHUB.sNeiCode AS TransactionCode,        
 cstmrIn.sCIN as  CinIn
, cstmrIn.sLastName as lastNameIn
, cstmrIn.sFirstName as FirstNameIn ,
replace(TAIHUB.nAmount ,'.' ,',') AS montant, 
CONVERT(VARCHAR, cashout.dInsertion, 103) + ' ' + CONVERT(VARCHAR, cashout.dInsertion, 108) AS dateout,
        outShop.sShopId AS shopout, 
        outShop.sName AS shopnameOut, 
 
 CityOut.sname as CityOut ,
 cstmrOut.sCIN as  CinOut
,cstmrOut.sLastName as lastNameOut
, cstmrOut.sFirstName as FirstNameOut 
, product.Sname
, TAIHUB.nTransactionsId
    from TAIHUB  

   left join Customer as cstmrIn on TAIHUB.nsenderid =  cstmrIn.nid
    left join Transactions as cashout
