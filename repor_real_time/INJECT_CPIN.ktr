tr.nproductId as ProductID,
'CPIN'as Product ,
 cast(tr.dInsertion as date) as 'date', 
 YEAR(cast(tr.dInsertion as date)) as 'annee',
 month(cast(tr.dInsertion as date)) as 'mois', 
DATEPART(wk ,cast(tr.dInsertion as date)) as "semaine",
DATEPART(dw ,cast(tr.dInsertion as date)) as "jour_semaine",
 cast(tr.dInsertion as time) as 'Heure', 
tr.nId as transaction_code ,
'' as sender_CIN, 
'' as Sender_firstname,
'' as Sender_lastname,
'' as sender_fidelity,
'' as senderStatus,
'' as Receiver_CIN,
'' as Receiver_firstname,
'' as Receiver_lastname,
'' as Receiver_fidelity,
'' as  ReceiverStatus,
tr.nAmount as amount,
tr.nOperationFee as OperationFee,
tr.nServiceFee as ServiceFee,
tr.nTransactionStatusId as TransactionStatutsCode,
upper(t.sName) as transactionStatuts,
sh.sShopId as Shop_Code,
Ci.sName as shop_Ville,
sh.sName as shop_name,
Com.sName as CompanyName,
cast(sh.bfranchise as varchar(50)) as reseau,
sh.nCompanyId as CompanyId,
'TRANSFERT' as TransactionType , 
'NATIONAL' as TransactionTypeDetail,
tr.nSenderId as nSenderId 
,tr.nReceiverId as nReceiverId
,1 as real_updated
,'' as date_customer_sender
, '' as date_customer_receiver
from 
Transactions "tr"   
left join 
            Shop "sh" on  cast(tr.nShopId as varchar(50))  = cast(sh.nId as varchar(50))  
left join  
            city "ci" on  cast(sh.nCityId as varchar(50))  =  cast(ci.nId as  varchar(50)) 
left join
            company  "com"  on cast(sh.nCompanyId as varchar(50))  =  cast(com.nId as varchar(50))
left  join 
            TANHUB  "tan"   on  tr.nId   = tan.nTransactionCashInId 
left join  
            product "p"  on   tr.nproductId = p.nId
left  join  
			TransactionStatus "t" on  tr.nTransactionStatusId = t.nId
where 
tr.nProductId = 1 and (tan.bpaid = 1 OR (tan.bPaid=0 and tan.bCancel=0))
and tr.nid > isnull(?,0) and cast(tr.dInsertion as date) = cast(getdate() as date)
order by cast(tr.nId as bigint);</sql>
