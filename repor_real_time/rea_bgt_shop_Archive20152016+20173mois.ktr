    <sql>&#xd;&#xa;use CASH_PLUS_DW&#xd;&#xa;SELECT date,produit as product,code, sum&#x28;nombre&#x29; as nbr_r,sum&#x28;montant&#x29; as volume_r, sum&#x28;CA&#x29; as CA_r&#xd;&#xa;from euro_cube_copy&#xd;&#xa;where produit in  &#x28;&#x27;CPIN&#x27;,&#x27;CPOUT&#x27;,&#x27;WU&#x27;,&#x27;MG&#x27;,&#x27;RIA&#x27;,&#x27;MT&#x27;,&#x27;ME&#x27;,&#x27;RT&#x27;,&#x27;VI&#x27;,&#x27;ASQ&#x27;,&#x27;VPS&#x27;&#x29; and year&#x28;date&#x29;&#x3d;2017 and month&#x28;date&#x29; in &#x28;4&#x29;&#xd;&#xa;group by date,code,produit&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;union &#xd;&#xa;&#xd;&#xa;select t.date as date,&#x27;AUTRE&#x27; as product ,t.code,sum&#x28;t.nbr_r&#x29; AS nbr_r,sum&#x28;t.volume_r&#x29; AS volume_r,sum&#x28;t.CA_r&#x29; as CA_r&#xd;&#xa;&#xd;&#xa;from &#x28;SELECT f.date ,f.produit ,f.code as code , sum&#x28;f.montant&#x29; as volume_r , sum&#x28;f.nombre&#x29; as nbr_r, sum&#x28;CA&#x29; as CA_r&#xd;&#xa;FROM dbo.euro_cube_copy f&#xd;&#xa;where f.produit in &#x28;&#x27;MEP&#x27;,&#x27;MI&#x27;,&#x27;WARI&#x27;,&#x27;SW&#x27;,&#x27;MONTY&#x27;,&#x27;BLM&#x27;,&#x27;PINTAIL&#x27;,&#x27;EUROPHIL&#x27;,&#x27;TZ&#x27;,&#x27;TITANES&#x27;,&#x27;MUNDI&#x27;,&#x27;LINK&#x27;,&#x27;FE&#x26;ZE&#x27;,&#x27;VLT&#x27;,&#x27;SF&#x27;&#x29;&#xd;&#xa;and year&#x28;date&#x29;&#x3d;2017 and month&#x28;date&#x29; in &#x28;4&#x29;&#xd;&#xa;group by f.date,f.code,f.produit&#xd;&#xa;&#x29; t&#xd;&#xa;group by t.date,t.code&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;union &#xd;&#xa;&#xd;&#xa;select date,&#x27;FACTURE&#x27; as product ,code, sum&#x28;f.nombre&#x29; as nbr_r,sum&#x28;f.montant&#x29; as volume_r , sum&#x28;CA&#x29;  as CA_r&#xd;&#xa;FROM dbo.euro_cube_copy f&#xd;&#xa;&#xd;&#xa;where f.produit  in &#x28;&#x27;TGR&#x27;,&#x27;AMN Tanger&#x27;,&#x27;AMN Tetouan&#x27;,&#x27;DGI&#x27;,&#x27;LYDEC&#x27;,&#x27;INWI FACTURE&#x27;,&#x27;RAK&#x27;,&#x27;RADEEMA&#x27;,&#x27;RAMSA&#x27;,&#x27;REDAL&#x27;&#x29; and year&#x28;date&#x29;&#x3d;2017 and month&#x28;date&#x29; in &#x28;4&#x29;&#xd;&#