    <sql> 
 select cast(tr.dinsertion as date ) as 'date' , sneicode ,  sshopid , tr.namount
from 
Transactions "tr"   
 inner join 
            taihub  "tai"   on  tr.nId   = tai.nTransactionsid 
left join shop
on nshopid = shop.nid

