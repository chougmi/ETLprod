    <connection>Prod</connection>
    <sql>

select
p.sname as 'Product' ,
cast(tr.dInsertion as date) as 'Date' ,


sh.sShopId as Shop_Code,

sh.sName as shop_name ,

count(*) as nbr , 
sum(tr.nAmount) as amount




from 
Transactions "tr"   
left join 
            Shop "sh" on  cast(tr.nShopId as varchar(50))  = cast(sh.nId as varchar(50))  
left join  
            city "ci" on  cast(sh.nCityId as varchar(50))  =  cast(ci.nId as  varchar(50)) 
left join
            company  "com"  on cast(sh.nCompanyId as varchar(50))  =  cast(com.nId as varchar(50))
left  join 
            TAIHUB  "tai"   on  tr.nId   = tai.nTransactionsId 
left join  
            product "p"  on   tr.nproductId = p.nId
left  join  
                  TransactionStatus "t" on  tr.nTransactionStatusId = t.nId
where  
      
tr.nProductId  in ( 5 , 6)  and cast(tr.dInsertion as date ) between '2017-01-01' and '2017-12-31' 
and len(sh.sShopId ) > 4

group by 

p.sname ,
cast(tr.dInsertion as date)  ,
sh.sShopId ,

sh.sName 




</sql>
